{{#articles}}
<article>
    <div class="article-title">{{title}}</div>
    <div class="article-body">{{{body}}}</div>
    <div class="article-tags">Tagi: {{#tags}}<div class="article-tag">{{tag}}</div>{{/tags}}</div>
    <div class="article-created">Opublikowano: <div class="article-created-date">{{created}}</div></div>
</article>
{{/articles}}